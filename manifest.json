{
    "manifest_version": 3,
    "name": "PobreTV AutoPlay",
    "description": "Auto play next episode on pobre tv",
    "version": "0.0",
    "background": {
        "service_worker":"event-handler.js"
    },
    "web_accessible_resources": [{
      "matches": [
        "https://*.pobre.wtf/*"
      ],
      "resources": ["next-episode.html"]
    }],
    "content_scripts": [
        {
          "js": ["scripts/episode-list.js", "scripts/frame.js"],
          "matches": [
            "https://*.pobre.wtf/*"
          ],
          "all_frames": true
        }
      ]
  }